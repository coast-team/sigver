{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "outDir": "dist/",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src/**/*"],
  "exclude": ["src/proto/**/*", "**/Message.d.ts", "src/sseserver_depricated/*"]
}
