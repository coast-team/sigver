{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES2015",
    "outDir": "dist/",
    "lib": ["ES2017"],
    "moduleResolution": "node",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": ["src/proto/**/*", "**/Message.d.ts", "src/sseserver_depricated/*"]
}
