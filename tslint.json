{
  "extends": [
    "tslint-eslint-rules",
    "tslint:recommended"
  ],
  "rules": {
    "no-console": false,
    "no-var-requires": false,
    "max-classes-per-file": false,
    "member-access": false,
    "no-default-export": true,
    "no-empty": false,
    "no-switch-case-fall-through": true,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "space-before-function-paren": true,
    "ter-indent": [true, 2],
    "no-string-literal": false,
    "variable-name": {
      "options": [
          "ban-keywords",
          "check-format",
          "allow-pascal-case",
          "allow-leading-underscore"
      ]
    }
  }
}
